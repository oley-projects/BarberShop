			<th>Время</th>
			<th>Парикмахер</th>
			<th>Цвет</th>
		</tr>
	</thead>
	<% @res.each do |row| %>
	<tbody>
	 	<tr>
	  		<td><%= row['username'] %></td>
	  		<td><%= row['phone'] %></td>
	  		<td><%= row['datestamp'] %></td>
	  		<td><%= row['barber'] %></td>
	  		<td><%= row['color'] %></td>
	 	</tr>
	</tbody>
	<% end %>
</table>